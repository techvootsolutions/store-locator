{
    "name": "techvoot/module-retailer",
    "type": "magento2-module",
    "version": "0.0.1",
    "description": "Techvoot Retailer Suite - Retailer Module",
    "keywords": ["magento2", "retailer suite", "retailer", "shop"],
    "authors": [
        {
            "name": "Techvoot Solutions",
            "email": "hello@techvoot.com"
        },
    ],
    "require": {
        "php": "^8.1",
        "magento/framework": ">=103.0.4",
        "magento/module-eav": ">=102.1.4",
        "magento/module-store": ">=101.1.4"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Techvoot\\Retailer\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    }
}
